{
  "apps": [
    {
      "name": "couchdb",
      "interpreter": "none",
      "script": "/docker-entrypoint.sh",
      "args": "/couchdb/bin/couchdb",
      "olde_args": "-n -a /etc/couchdb/default.ini -z couch-config/couch.prod.ini",
      "kill_timeout": 5
    },
    {
      "name": "clouseau1",
      "interpreter": "none",
      "script": "mvn",
      "args": "scala:run -Dlauncher=clouseau1 -DFOREGROUND",
      "cwd": "/clouseau",
      "kill_timeout": 5
    },
    {
      "name": "clouseau2",
      "interpreter": "none",
      "script": "mvn",
      "args": "scala:run -Dlauncher=clouseau2 -DFOREGROUND",
      "cwd": "/clouseau",
      "kill_timeout": 5
    },
    {
      "name": "clouseau3",
      "interpreter": "none",
      "script": "mvn",
      "args": "scala:run -Dlauncher=clouseau3 -DFOREGROUND",
      "cwd": "/clouseau",
      "kill_timeout": 5
    }
  ]
}
